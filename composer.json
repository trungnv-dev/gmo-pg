{
    "name": "gmo-ecs/gmo-pg",
    "description": "Core Gmo Payment Services",
    "keywords": ["gmo", "ecs", "laravel", "php", "gmo-pg"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Trungnv",
            "email": "trungnv@runsystem.net"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Ecs\\GmoPG\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ecs\\GmoPG\\Providers\\GmoPGServiceProvider"
            ],
            "options": {
                "payment_type": "credit_card"
            }
        }
    }
}